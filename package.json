{
  "name": "My-server",
  "version": "0.1.0",
  "description": "Express TypeScript Mongoose (MongoDB) Server",
  "scripts": {
    "dev": "nodemon",
    "start": "node dist/server.js",
    "deploy": "nhb-build && vercel --prod",
    "build": "nhb-build",
    "format": "nhb-format",
    "lint": "nhb-lint",
    "fix": "nhb-fix",
    "commit": "nhb-commit",
    "count": "nhb-count",
    "delete": "nhb-delete",
    "module": "nhb-module -t express-mongoose-zod -d src/app/modules",
    "secret": "node secret.mjs"
  },
  "author": {
    "name": "Nazmul Hassan",
    "email": "nazmulnhb@gmail.com"
  },
  "license": "ISC",
  "keywords": [
    "My-server",
    "server",
    "express",
    "typescript",
    "mongoose"
  ],
  "dependencies": {
    "bcrypt": "^6.0.0",
    "cloudinary": "^2.8.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.19.3",
    "multer": "^2.0.2",
    "nhb-toolbox": "^4.25.4",
    "nodemailer": "^7.0.10",
    "serve-favicon": "^2.5.1",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@types/bcrypt": "^6.0.0",
    "@types/cookie-parser": "^1.4.10",
